{
  "name": "Snapshot as HTML",
  "description": "A chrome extension save webpage snapshot as html",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["tabs", "<all_urls>", "activeTab"],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}